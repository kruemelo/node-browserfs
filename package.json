{
  "name": "node-browserfs",
  "version": "2.1.0",
  "description": "AMD/CommonJS module providing a node fs-like browser in-memory file system ",
  "main": "browserfs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kruemelo/node-browserfs.git"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "minify": "^2.0.12",
    "mocha": "^2.4.5",
    "requirejs": "^2.2.0"
  },
  "keywords": [
    "node",
    "fs",
    "browser"
  ],
  "scripts": {
    "main": "browserfs.js",
    "test": "mocha",
    "build": "npm test && npm run browserify && npm run minify",
    "browserify": "browserify ./lib/browserfs.js -s BrowserFs > ./browserfs.js",
    "minify": "./node_modules/.bin/minify ./browserfs.js > ./browserfs.min.js"
  },
  "author": "kruemelo",
  "license": "WTFPL",
  "dependencies": {}
}
